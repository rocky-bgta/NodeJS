{
  "name": "auditLog",
  "version": "0.0.1",
  "description": "Audit Log Module",
  "repository": "",
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "run": "npm run build && npm run start",
    "start": "./node_modules/.bin/ts-node src/index.ts",
    "debug": "npm run build && npm run support_debug",
    "support_debug": "./node_modules/.bin/ts-node %NODE_DEBUG_OPTION% src/index.ts",
    "start:cluster": "./node_modules/.bin/ts-node src/cluster.ts",
    "build": "npm run clean && tsc && npm run swagger",
    "clean": "rimraf dist",
    "delete_Node_Module": "rimraf node_modules",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "format": "tsfmt -r",
    "pretest": "cross-env NODE_ENV=test npm build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha",
    "test:coverage": "nyc npm test",
    "swagger": "swaggerGen -c ./swagger.config.json",
    "tsc": "tsc && npm run swagger",
    "watch": "./node_modules/.bin/ts-node-dev dist/index.js"
  },
  "dependencies": {
    "@types/compression": "0.0.34",
    "@types/datejs": "0.0.30",
    "@types/dotenv": "^4.0.2",
    "@types/helmet": "0.0.36",
    "@types/knex": "0.0.62",
    "@types/morgan": "^1.7.34",
    "@types/winston": "^2.3.7",
    "casting": "0.0.2",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^8.1.0",
    "jsonwebtoken-refresh": "^5.7.0",
    "knex": "^0.13.0",
    "morgan": "^1.9.0",
    "nodemailer": "^4.3.1",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "pg-pool": "^2.0.3",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^3.30.4",
    "sequelize-typescript": "^0.5.0",
    "string-builder": "^0.1.4",
    "ts-md5": "^1.2.2",
    "ts-node-dev": "^1.0.0-pre.5",
    "typescript-ioc": "^1.0.2",
    "typescript-rest": "^1.1.1",
    "typescript-rest-swagger": "0.0.10",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/cors": "^2.8.1",
    "@types/mocha": "^2.2.41",
    "@types/request": "0.0.43",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-file-cache": "0.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rimraf": "^0.2.1",
    "gulp-run": "^1.7.1",
    "gulp-sequence": "^0.4.6",
    "gulp-task": "^2.1.0",
    "gulp-typescript": "^3.2.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "nyc": "^10.3.2",
    "request": "^2.83.0",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.15",
    "ts-node": "~3.0.6",
    "tslint": "~5.4.3",
    "typedoc": "^0.5.10",
    "typescript": "~2.3.4",
    "typescript-formatter": "^5.2.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
